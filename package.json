{
  "name": "Express-scaffold",
  "version": "1.0.0",
  "description": "Scaffold for express/node/mongo apps",
  "main": "server.js",
  "scripts": {
    "dev": "npm run test && nodemon -w src --exec \"babel-node src/server\"",
    "start": "node dist",
    "test": "npm run lint && NODE_ENV=test mocha --require babel-core/register --timeout 5000 src/tests/*.js",
    "build": "babel src --ignore '/tests/' -s -D -d dist",
    "lint": "eslint src"
  },
  "author": "DJ <jadczak.d@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.17.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "mongoose": "^5.0.3",
    "morgan": "^1.9.0",
    "nodemon": "^1.14.12",
    "path": "^0.12.7",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "dotenv": "^4.0.0",
    "eslint": "^4.9.0",
    "@std/esm": "^0.20.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^5.0.0",
    "request": "^2.83.0",
    "should": "^13.1.0"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  }
}
